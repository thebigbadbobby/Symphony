{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "scripts": {
    "client-install": "cd client && npm install",
    "client-test": "cd client && npm run test",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "twillio-webhook": "cd twillio && twilio phone-numbers:update \"+14302390178\n\" --sms-url=\"http://localhost:1337/sms\"",
    "twillio-server": "cd twillio && node server.js",
    "twillio": "concurrently \"npm run twillio-webhook\" \"npm run twillio-server\"",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "author": "Kahzum",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "react-loading": "^2.0.3",
    "twilio": "^3.49.4"
  }
}

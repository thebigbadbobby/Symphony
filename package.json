{
  "name": "reactexpress",
  "version": "1.0.0",
  "description": "Starter kit for creating React and Express apps",
  "main": "server.js",
  "scripts": {
    "update": "npm install && cd client && npm install",
    "client-install": "cd client && npm install",
    "client-test": "cd client && npm run test",
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm start --prefix client",
    "twilio-webhook": "twilio phone-numbers:update \"+14085210010\n\" --sms-url=\"http://localhost:5000/twilio/sms\"",
    "dev": "concurrently \"npm run server\" \"npm run client\" \"npm run twilio-webhook\"",
    "kill": "killall node",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "twilio-prod": "twilio phone-numbers:update \"+14302390178\n\" --sms-url=\"https://api.kahzum.com/twilio/sms\"",
    "docker": "docker-compose up -d",
    "prod": "concurrently \"npm run server\" \"npm run twilio-prod\""
  },
  "author": "Kahzum",
  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0"
  },
  "dependencies": {
    "axios": "^0.21.0",
    "body-parser": "^1.19.0",
    "concurrently": "^3.5.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "mongodb": "^3.6.2",
    "mongoose": "^5.10.9",
    "twilio": "^3.49.4",
    "twilio-cli": "^2.12.0"
  },
  "nodemonConfig": {
    "verbose": true,
    "ignore": [
      "/routing/dailyDestinationList/*.json"
    ]
  }
}